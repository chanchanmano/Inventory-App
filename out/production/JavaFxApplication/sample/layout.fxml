<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="575.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Pane prefHeight="36.0" prefWidth="600.0" />
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="191.0" prefWidth="572.0" AnchorPane.topAnchor="55.0">
         <children>
            <VBox prefHeight="169.0" prefWidth="249.0">
               <children>
                  <TextField fx:id="nameProd" promptText="Enter Name Of Product">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <TextField fx:id="priceProd" promptText="Enter Price (p)">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <TextField fx:id="quanProd" promptText="Enter Quantity(q)">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <Button mnemonicParsing="false" onAction="#addProdB" prefHeight="26.0" prefWidth="114.0" text="Add Product">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#shipProdB" prefHeight="26.0" prefWidth="114.0" text="Ship Product">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                     </VBox.margin>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </VBox>
            <VBox prefHeight="155.0" prefWidth="406.0">
               <children>
                  <TextField fx:id="infoProd" promptText="Product Name To View Info">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <Button mnemonicParsing="false" onAction="#searchProdB" text="Search">
                     <VBox.margin>
                        <Insets bottom="10.0" left="140.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Label fx:id="labelInfo" prefHeight="110.0" prefWidth="272.0" text="Label">
                     <VBox.margin>
                        <Insets left="50.0" right="50.0" top="15.0" />
                     </VBox.margin>
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
      <Pane layoutX="15.0" layoutY="225.0" prefHeight="200.0" prefWidth="570.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="300.0">
         <children>
            <ListView fx:id="listProd" layoutX="133.0" layoutY="2.0" prefHeight="198.0" prefWidth="304.0" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </Pane>
      <Label fx:id="labelprofloss" layoutX="203.0" layoutY="513.0" text="Label" />
      <Label fx:id="labeldisplayprofloss" layoutX="314.0" layoutY="513.0" text="Label" />
      <Label fx:id="inventory" layoutX="203.0" layoutY="544.0" text="Label" />
      <Label fx:id="displayinventory" layoutX="314.0" layoutY="544.0" text="Label" />
   </children>
</AnchorPane>
